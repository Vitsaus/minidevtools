{
  "name": "@vitsaus/minitools",
  "version": "0.0.1",
  "description": "small electron app with usefull little minitools",
  "productName": "@vitsaus/mini-tools",
  "main": "dist/container.js",
  "scripts": {
    "preelectron": "npm run build:electron:prod",
    "watch": "nodemon --config ./nodemon.json",
    "electron": "electron ./dist/container.js",
    "start:electron": "electron ./src/container/index.js",
    "start:electron:prod": "electron ./dist/container.js",
    "start:app": "webpack --color --mode development --progress --config webpack.config.app.js",
    "build:app": "webpack --color --mode development --config webpack.config.app.js",
    "build:app:prod": "webpack --color --mode production --config webpack.config.app.prod.js",
    "build:electron": "webpack --color --mode development --config webpack.config.electron.js",
    "build:electron:prod": "webpack --color --mode production --config webpack.config.electron.prod.js",
    "package:mac": "electron-packager . minitools --overwrite --asar --platform=darwin --arch=x64 --prune=true --out=release-builds --app-bundle-id=com.vitsaus.minidevtools"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
